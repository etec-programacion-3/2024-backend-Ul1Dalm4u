Base de datos comandos:

CREATE DATABASE tino_uli;
USE tino_uli
CREATE TABLE products (
  id INT AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(255) NOT NULL,
  description TEXT NOT NULL,
  price FLOAT NOT NULL,
  stock INT NOT NULL
);
SHOW DATABASES;

